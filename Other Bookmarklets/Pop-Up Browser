javascript:(function() {  var overlay = document.createElement('div');  overlay.style.position = 'fixed';  overlay.style.top = '0';  overlay.style.left = '0';  overlay.style.width = '100%';  overlay.style.height = '100%';  overlay.style.background = 'rgba(0, 0, 0, 0.7)';  overlay.style.zIndex = '9999999';  overlay.style.opacity = '0';  overlay.style.transition = 'opacity 0.3s ease-in-out';  overlay.style.display = 'flex';  overlay.style.alignItems = 'center';  overlay.style.justifyContent = 'center';  overlay.style.borderRadius = '10px';  var iframe = document.createElement('iframe');  iframe.src = 'https://www.bing.com';  iframe.style.width = '80vw';  iframe.style.height = '80vh';  iframe.style.borderRadius = '10px';  overlay.appendChild(iframe);  document.body.appendChild(overlay);  setTimeout(function() {    overlay.style.opacity = '1';  }, 100);  function closeOverlay() {    overlay.style.opacity = '0';    setTimeout(function() {      document.body.removeChild(overlay);    }, 300);  }  document.addEventListener('keydown', function(event) {    if (event.key === 'Escape') {      closeOverlay();    }  });  overlay.addEventListener('click', closeOverlay);})();
